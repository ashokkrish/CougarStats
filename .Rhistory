## Mammalian Sleep  ----
msleep %>%
drop_na(vore) %>%       # drop missing values
group_by(vore) %>%      # group by vore categories
summarise(Lower = min(sleep_total),
Average = mean(sleep_total),
Upper = max(sleep_total),
Difference = Upper - Lower) %>%
arrange(Average) %>%
